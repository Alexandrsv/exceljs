{
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2022
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "arrow-parens": "off",
    "indent": ["warn",2],
    "require-jsdoc": "off"
  },
  "extends": [
    "eslint:recommended",
    "google",
    "prettier"
  ]
}
